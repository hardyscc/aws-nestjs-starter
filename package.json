{
  "name": "aws-nestjs-starter",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:watch": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:online": "cross-env IS_OFFLINE=false nest start",
    "deploy": "sls deploy --verbose",
    "package": "sls package",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "dbb:install": "sls dynamodb install",
    "dbb:start": "sls dynamodb start",
    "genres": "ts-node genres.ts -- 'src/**/*.schema.ts' 'resources/dynamodb.yml'"
  },
  "dependencies": {
    "@nestjs/common": "7.0.8",
    "@nestjs/config": "0.4.0",
    "@nestjs/core": "7.0.8",
    "@nestjs/graphql": "7.3.1",
    "@nestjs/platform-express": "7.0.8",
    "apollo-server-express": "2.12.0",
    "aws-serverless-express": "3.3.8",
    "dynamoose": "2.0.0-beta.10",
    "graphql": "14.6.0",
    "graphql-tools": "5.0.0",
    "nestjs-dynamoose": "0.1.3",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.5.5",
    "type-graphql": "0.17.6",
    "uuid": "7.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@nestjs/cli": "7.1.2",
    "@nestjs/schematics": "7.0.0",
    "@nestjs/testing": "7.0.8",
    "@shelf/jest-dynamodb": "1.7.0",
    "@types/aws-lambda": "8.10.50",
    "@types/aws-serverless-express": "3.3.3",
    "@types/express": "4.17.4",
    "@types/jest": "25.2.1",
    "@types/js-yaml": "3.12.3",
    "@types/node": "13.13.0",
    "@types/serverless": "1.67.0",
    "@types/supertest": "2.0.8",
    "@types/uuid": "7.0.2",
    "@typescript-eslint/eslint-plugin": "2.28.0",
    "@typescript-eslint/parser": "2.28.0",
    "change-case": "4.1.1",
    "cross-env": "7.0.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.2",
    "glob-promise": "3.4.0",
    "husky": "4.2.5",
    "jest": "25.3.0",
    "js-yaml": "3.13.1",
    "lint-staged": "10.1.5",
    "prettier": "2.0.4",
    "serverless": "1.67.3",
    "serverless-dynamodb-local": "0.2.39",
    "serverless-plugin-typescript": "1.1.9",
    "supertest": "4.0.2",
    "ts-jest": "25.4.0",
    "ts-loader": "7.0.0",
    "ts-node": "8.8.2",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.8.3"
  },
  "jest": {
    "preset": "@shelf/jest-dynamodb",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "testTimeout": 45000
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
